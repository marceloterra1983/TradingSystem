{
  "version": "1.0.0",
  "mappings": [
    {
      "id": "workspace-api-routes",
      "source": {
        "type": "backend-api",
        "paths": [
          "backend/api/workspace/src/routes/**/*.{js,ts}"
        ],
        "triggers": [
          "router.get",
          "router.post",
          "router.put",
          "router.delete"
        ]
      },
      "targets": [
        {
          "path": "docs/content/api/workspace-api.mdx",
          "sections": [
            "Main Endpoints",
            "Endpoint Details"
          ]
        }
      ],
      "severity": "critical",
      "owner": "BackendGuild",
      "autoUpdate": false
    },
    {
