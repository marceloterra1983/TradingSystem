      "source": {
        "type": "env-config",
        "paths": [
          "backend/api/workspace/src/config/**/*.{js,ts}",
          ".env.example"
        ],
        "triggers": [
          "process.env.",
          "export const",
          "="
        ]
      },
      "targets": [
        {
          "path": "docs/content/api/workspace-api.mdx",
          "sections": [
