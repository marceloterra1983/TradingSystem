    {
      "id": "tp-capital-app-config",
      "source": {
        "type": "app-code",
        "paths": [
          "apps/tp-capital/src/**/*.{js,ts,tsx}",
          "apps/tp-capital/package.json"
        ],
        "triggers": [
          "PORT",
          "DEFAULT",
          "\"version\":"
        ]
      },
      "targets": [
        {
          "path": "docs/content/apps/tp-capital/overview.mdx",
          "sections": [
            "Service Details",
            "Configuration"
