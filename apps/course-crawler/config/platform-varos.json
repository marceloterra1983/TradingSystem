{
  "login": {
    "url": "https://plataforma.varos.com.br/auth/login",
    "usernameSelector": "input[type=\"email\"]",
    "passwordSelector": "input[type=\"password\"]",
    "submitSelector": "button[type=\"submit\"]",
    "postLoginWaitSelector": "a[href*=\"/area/\"], div[class*=\"dashboard\"], nav[class*=\"menu\"], a[href*=\"perfil\"]"
  },
  "courses": {
    "url": "https://plataforma.varos.com.br/area/vitrine",
    "courseItemSelector": "div[class*=\"course\"], div[class*=\"card\"], a[href*=\"/curso/\"]",
    "titleSelector": "h1, h2, h3, [class*=\"title\"], [class*=\"nome\"]",
    "linkSelector": "a[href*=\"/curso/\"], a[href*=\"/area/\"]"
  },
  "modules": {
    "moduleListSelector": "div[class*=\"module\"], div[class*=\"modulo\"], div[class*=\"section\"], ul li",
    "titleSelector": "h2, h3, h4, [class*=\"title\"], [class*=\"nome\"]",
    "linkSelector": "a"
  },
  "classes": {
    "classListSelector": "div[class*=\"lesson\"], div[class*=\"aula\"], div[class*=\"class\"], li",
    "titleSelector": "span, a, [class*=\"title\"], [class*=\"nome\"]",
    "linkSelector": "a",
    "contentSelector": "div[class*=\"content\"], div[class*=\"conteudo\"], div[class*=\"description\"], p",
    "attachmentSelector": "a[href*=\"download\"], a[href*=\".pdf\"], a[href*=\".zip\"], a[href*=\"anexo\"]",
    "videoSelector": "video, iframe[src*=\"player\"], iframe[src*=\"vimeo\"], iframe[src*=\"youtube\"], iframe[src*=\"wistia\"]",
    "durationSelector": "[class*=\"duration\"], [class*=\"duracao\"], [class*=\"time\"]"
  }
}
