{
  "login": {
    "url": "https://plataforma.varos.com.br/auth/login",
    "usernameSelector": "#AcessoEmail",
    "passwordSelector": "#AcessoSenha",
    "submitSelector": "form#Form button[type=\"submit\"]",
    "postLoginWaitSelector": "a[href*=\"/area/\"], div[class*=\"dashboard\"], nav[class*=\"menu\"], a[href*=\"perfil\"]"
  },
  "courses": {
    "url": "https://plataforma.varos.com.br/area/vitrine",
    "courseItemSelector": "div[class*=\"produto\"], article[class*=\"produto\"], li[class*=\"produto\"], a[href*=\"/area/produto\"], a[href*=\"/area/vitrine\"]",
    "titleSelector": "h1, h2, h3, strong, span, [class*=\"title\"], [class*=\"nome\"], [class*=\"titulo\"]",
    "linkSelector": "a[href*=\"/area/\"]"
  },
  "modules": {
    "moduleListSelector": ".sections .section-group:not(.progresso-total)",
    "titleSelector": ".section-group-titulo .item-titulo",
    "linkSelector": ".section-group-titulo a"
  },
  "classes": {
    "classListSelector": ".section-items .section-item",
    "titleSelector": ".item-titulo",
    "linkSelector": ".layer-link",
    "contentSelector": "div[class*=\"content\"], div[class*=\"conteudo\"], div[class*=\"description\"], p",
    "attachmentSelector": "a[href*=\"download\"], a[href*=\".pdf\"], a[href*=\".zip\"], a[href*=\"anexo\"]",
    "videoSelector": "video, iframe[src*=\"player\"], iframe[src*=\"vimeo\"], iframe[src*=\"youtube\"], iframe[src*=\"wistia\"]",
    "durationSelector": "[class*=\"duration\"], [class*=\"duracao\"], [class*=\"time\"]"
  }
}
