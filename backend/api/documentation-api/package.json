{
  "name": "documentation-api",
  "version": "0.1.0",
  "description": "Documentation management API for TradingSystem",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "NODE_ENV=development node --watch src/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "docs:check": "node src/scripts/runHealthCheck.js",
    "docs:watch": "nodemon --watch ../../docs/spec --ext yaml,json --exec npm run docs:check",
    "migrate:questdb:postgres": "node src/scripts/migrate-questdb-to-postgres.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev --name init",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "semantic:index": "node src/scripts/index-docs-to-qdrant.js",
    "semantic:index:llama": "node src/scripts/index-docs-to-qdrant-llamaindex.js",
    "semantic:index:auto": "node -e \"(process.env.SEMANTIC_INDEXER==='llamaindex'?await import('./src/scripts/index-docs-to-qdrant-llamaindex.js'):await import('./src/scripts/index-docs-to-qdrant.js'))\""
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "@asyncapi/parser": "^3.4.0",
    "@prisma/client": "^6.17.1",
    "@qdrant/js-client-rest": "^1.9.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "archiver": "^5.3.1",
    "axios": "^1.12.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "flexsearch": "^0.7.31",
    "generic-pool": "^3.9.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "isomorphic-dompurify": "^2.14.0",
    "js-yaml": "^4.1.0",
    "llamaindex": "^0.3.11",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "pino": "^9.4.0",
    "pino-pretty": "^13.1.2",
    "prom-client": "^15.1.0",
    "semver": "^7.5.4"
  },
  "license": "MIT",
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "nodemon": "^3.1.0",
    "prisma": "^6.17.1",
    "supertest": "^7.1.4",
    "vitest": "^3.2.4"
  }
}
