# Slack incoming webhook (optional)
DOCS_GUARD_SLACK_WEBHOOK="https://hooks.slack.com/services/XXXX/XXXX/XXXX"

# Sentry configuration (optional)
DOCS_GUARD_SENTRY_DSN="https://public@example.ingest.sentry.io/123456"
DOCS_GUARD_SENTRY_ENV="production"
DOCS_GUARD_SENTRY_RELEASE="docs-hub-guard@1.0.0"
