{
  "dashboard": {
    "title": "Service Launcher - Monitoring Dashboard",
    "tags": ["service-launcher", "health", "monitoring"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Overall System Status",
        "type": "stat",
        "gridPos": { "x": 0, "y": 0, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "service_launcher_overall_status",
            "legendFormat": "Status"
          }
        ],
        "options": {
          "graphMode": "none",
          "colorMode": "background",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              { "type": "value", "value": "1", "text": "OK", "color": "green" },
              { "type": "value", "value": "0.5", "text": "DEGRADED", "color": "yellow" },
              { "type": "value", "value": "0", "text": "DOWN", "color": "red" }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "red" },
                { "value": 0.5, "color": "yellow" },
                { "value": 1, "color": "green" }
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Service Health Status",
        "type": "stat",
        "gridPos": { "x": 6, "y": 0, "w": 18, "h": 4 },
        "targets": [
          {
            "expr": "service_launcher_service_status",
            "legendFormat": "{{service_name}}"
          }
        ],
        "options": {
          "graphMode": "none",
          "colorMode": "background",
          "orientation": "horizontal"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              { "type": "value", "value": "1", "text": "OK", "color": "green" },
              { "type": "value", "value": "0.5", "text": "DEGRADED", "color": "yellow" },
              { "type": "value", "value": "0", "text": "DOWN", "color": "red" }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "red" },
                { "value": 0.5, "color": "yellow" },
                { "value": 1, "color": "green" }
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Health Check Latency (95th percentile)",
        "type": "graph",
        "gridPos": { "x": 0, "y": 4, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(service_launcher_health_check_duration_seconds_bucket[5m]))",
            "legendFormat": "{{service_id}}"
          }
        ],
        "yaxes": [
          { "label": "Latency (seconds)", "format": "s" },
          { "show": false }
        ]
      },
      {
        "id": 4,
        "title": "Health Check Success Rate",
        "type": "graph",
        "gridPos": { "x": 12, "y": 4, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "rate(service_launcher_health_check_total{status=\"ok\"}[5m]) / rate(service_launcher_health_check_total[5m])",
            "legendFormat": "{{service_id}}"
          }
        ],
        "yaxes": [
          { "label": "Success Rate", "format": "percentunit", "max": 1, "min": 0 },
          { "show": false }
        ]
      },
      {
        "id": 5,
        "title": "Circuit Breaker Status",
        "type": "stat",
        "gridPos": { "x": 0, "y": 12, "w": 8, "h": 4 },
        "targets": [
          {
            "expr": "service_launcher_circuit_breaker_state",
            "legendFormat": "{{service_id}}"
          }
        ],
        "options": {
          "graphMode": "none",
          "colorMode": "background"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              { "type": "value", "value": "0", "text": "CLOSED", "color": "green" },
              { "type": "value", "value": "1", "text": "OPEN", "color": "red" }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 1, "color": "red" }
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Circuit Breaker Failures",
        "type": "graph",
        "gridPos": { "x": 8, "y": 12, "w": 16, "h": 4 },
        "targets": [
          {
            "expr": "service_launcher_circuit_breaker_failures",
            "legendFormat": "{{service_id}}"
          }
        ],
        "yaxes": [
          { "label": "Failure Count", "format": "short" },
          { "show": false }
        ]
      },
      {
        "id": 7,
        "title": "Service Launch Operations",
        "type": "graph",
        "gridPos": { "x": 0, "y": 16, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "rate(service_launcher_launch_total[5m])",
            "legendFormat": "{{service_name}} ({{method}}, {{status}})"
          }
        ],
        "yaxes": [
          { "label": "Launches/sec", "format": "ops" },
          { "show": false }
        ]
      },
      {
        "id": 8,
        "title": "Service Launcher Resource Usage",
        "type": "graph",
        "gridPos": { "x": 12, "y": 16, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "service_launcher_process_resident_memory_bytes / 1024 / 1024",
            "legendFormat": "Memory (MB)"
          },
          {
            "expr": "rate(service_launcher_process_cpu_seconds_total[1m]) * 100",
            "legendFormat": "CPU %"
          }
        ],
        "yaxes": [
          { "label": "Usage", "format": "short" },
          { "show": false }
        ]
      },
      {
        "id": 9,
        "title": "Health Checks per Service",
        "type": "table",
        "gridPos": { "x": 0, "y": 24, "w": 24, "h": 8 },
        "targets": [
          {
            "expr": "service_launcher_health_check_total",
            "format": "table",
            "instant": true
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "includeByName": {
                "service_id": true,
                "status": true,
                "Value": true
              },
              "renameByName": {
                "service_id": "Service",
                "status": "Status",
                "Value": "Total Checks"
              }
            }
          }
        ]
      }
    ]
  }
}














