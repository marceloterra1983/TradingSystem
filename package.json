{
  "type": "module",
  "devDependencies": {
    "@modelcontextprotocol/server-filesystem": "^2025.8.21",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "eslint": "^9.37.0",
    "glob": "^11.0.0",
    "husky": "^9.0.11",
    "markdown-it": "^14.1.0",
    "markdown-it-multimd-table": "^4.2.3",
    "@tktco/node-actionlint": "^1.6.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "compression": "^1.8.1",
    "dotenv": "^17.2.3",
    "express-rate-limit": "^8.1.0",
    "ioredis": "^5.3.2",
    "js-yaml": "^4.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "redis": "^4.6.13",
    "yaml": "^2.5.1"
  },
  "scripts": {
    "start": "bash scripts/start.sh --force-kill",
    "start:all": "bash scripts/start.sh --force-kill",
    "start:soft": "bash scripts/start.sh",
    "env:validate": "bash scripts/env/validate-env.sh",
    "env:validade": "npm run env:validate",
    "stop": "bash scripts/stop.sh",
    "stop:all": "bash scripts/stop.sh --with-db --prune-networks",
    "prepare": "husky install",
    "build": "bash scripts/build/parallel-build.sh",
    "build:clean": "bash scripts/build/parallel-build.sh --clean",
    "build:measure": "bash scripts/build/parallel-build.sh --measure",
    "build:sequential": "cd docs && npm run docs:build && cd ../frontend/dashboard && npm run build",
    "lint": "cd frontend/dashboard && npm run lint",
    "lint:fix": "eslint . --fix",
    "ci:lint-workflows": "bash scripts/ci/lint-workflows.sh",
    "type-check": "tsc --build",
    "type-check:dashboard": "cd frontend/dashboard && npm run type-check",
    "validate-docs": "python3 scripts/docs/validate-frontmatter.py --docs-dir ./docs/content",
    "agent:docs:daily": "node scripts/agents/docusaurus-daily.mjs",
    "docs:check-sync": "node scripts/agents/docusaurus-daily.mjs --check-sync",
    "docs:check-sync:critical": "node scripts/agents/docusaurus-daily.mjs --check-sync --severity-threshold critical",
    "docs:create-sync-pr": "node scripts/agents/docusaurus-daily.mjs --create-pr",
    "docs:validate-mapping": "jq . governance/registry/CODE-DOCS-MAPPING.json",
    "agent:list": "node scripts/agents/runner.mjs list",
    "agent:describe": "node scripts/agents/runner.mjs describe",
    "agent:run": "node scripts/agents/runner.mjs run",
    "agent:new": "node scripts/agents/new-agent.mjs",
    "dev:dashboard-docs": "bash scripts/docs/start-dashboard-with-docs.sh",
    "smoke:docsapi": "bash scripts/maintenance/smoke-docsapi.sh",
    "smoke:waha": "bash scripts/maintenance/smoke-waha.sh",
    "docs:restart": "bash scripts/maintenance/restart-docusaurus.sh",
    "dashboard:restart": "bash scripts/maintenance/restart-dashboard.sh",
    "dev:stack": "bash -c 'bash scripts/maintenance/restart-docusaurus.sh && bash scripts/maintenance/restart-dashboard.sh'",
    "rag:ask": "node scripts/rag/query-cli.mjs",
    "openspec": "node tools/openspec/cli.mjs",
    "commands:generate": "node scripts/commands/generate-commands-db.mjs",
    "commands:build": "npm run commands:generate",
    "commands:test": "node --test scripts/commands/__tests__/*.test.mjs",
    "commands:ci": "npm run commands:test && npm run commands:generate -- --ci",
    "ports:validate": "node tools/ports/validate.js",
    "ports:duplicates": "node tools/ports/validate.js --mode=duplicates",
    "ports:check-ranges": "node tools/ports/validate.js --mode=ranges",
    "ports:sync": "node tools/ports/sync.js",
    "ports:scan-hardcoded": "node tools/ports/scan-hardcoded.js",
    "ports:report": "node tools/ports/report.js",
    "ports:test": "node --test tools/ports/__tests__/*.test.js",
    "agents:generate": "node scripts/agents/generate-agents-directory.mjs",
    "agents:test": "node --test scripts/agents/__tests__/*.test.mjs",
    "agents:ci": "npm run agents:test && npm run agents:generate -- --ci",
    "codex:quality-check": "bash scripts/codex/run-command.sh quality-check",
    "codex:health-check": "bash scripts/codex/run-command.sh health-check",
    "codex:docker": "bash scripts/codex/run-command.sh docker",
    "codex:scripts": "bash scripts/codex/run-command.sh scripts",
    "commands:watch": "node scripts/commands/watch-commands.mjs",
    "governance:auto": "node governance/automation/generate-docs-index.mjs && node governance/automation/sync-docusaurus.mjs",
    "governance:metrics": "node governance/automation/governance-metrics.mjs",
    "governance:validate-policies": "node governance/automation/validate-policies.mjs",
    "governance:validate-envs": "node governance/automation/validate-envs.mjs",
    "governance:scan-secrets": "node governance/automation/scan-secrets.mjs",
    "governance:check": "npm run governance:validate-policies && npm run governance:validate-envs && npm run governance:scan-secrets",
    "governance:full": "node governance/automation/validate-registry.mjs && npm run governance:auto && npm run governance:metrics && npm run governance:check"
  }
}
