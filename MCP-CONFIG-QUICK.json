{
  "mcpServers": {
    "GitKraken": {
      "args": [
        "mcp",
        "--host=cursor",
        "--source=gitlens",
        "--scheme=cursor"
      ],
      "command": "/home/marce/.cursor-server/data/User/globalStorage/eamodio.gitlens/gk",
      "type": "stdio"
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/home/marce/Projetos/TradingSystem"
      ]
    },
    "docker": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-server-docker"
      ]
    },
    "npm": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-npm"
      ]
    },
    "github": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "SUBSTITUA_PELO_SEU_TOKEN_GITHUB"
      }
    },
    "postgres": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-postgres"
      ],
      "env": {
        "POSTGRES_CONNECTION_STRING": "postgresql://timescale:pass_timescale@localhost:5433/trading"
      }
    }
  }
}

